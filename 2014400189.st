Object subclass: #Dota	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Dota commentStamp: '<historical>' prior: 0!Represents the main class of the game.!Dota subclass: #GameArena	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!GameArena commentStamp: '<historical>' prior: 0!Main class of the game.!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!GameArena class	instanceVariableNames: ''!!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 22:57'!viewEnds: player1 and: player2	"prints the end of game"	self viewPlayers: player1 and: player2.	player1 fight hp > 0		ifTrue: [ self viewEnd: player1 ]		ifFalse: [ self viewEnd: player2 ]! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 13:47'!viewPlayer: player	"prints player information"	Transcript		show: player name;		cr;		show: 'Hero: ';		show: player fight hero name;		cr;		show: player fight hp asString;		show: ' / ';		show: player fight hero hp asString;		show: ' HP';		cr;		show: player fight mana asString;		show: ' / ';		show: player fight hero mana asString;		show: ' Mana';		cr;		show: 'Last Action: ';		show: player fight lastAction;		cr;		show: (self viewRemainingPotions: player);		cr;		show: (self viewSkillsInCooldown: player);		cr;		show: (self viewStunned: player);		cr! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:33'!viewEnd: player	"prints the winner of the game"	Transcript		show: player name;		show: ' (';		show: player fight hero name;		show: ') is the winner of the game !!!!!!'! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:30'!viewPlayers: player1 and: player2	"prints players information"	self viewPlayer: player1.	self viewPlayer: player2! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 22:36'!viewSkillsInCooldown: player	"returns string containing information about skills in cooldown"	| skill1 skill2 message |	message := ''.	skill1 := player fight hero skill1.	skill1 count > 0		ifTrue: [ message := message , skill1 name , ' (not available for ' , skill1 count asString , ' turns)' ].	skill2 := player fight hero skill2.	skill2 count > 0		ifTrue: [ skill1 count > 0				ifTrue: [ message := message , ', ' ].			message := message , skill2 name , ' (not available for ' , skill2 count asString , ' turns)' ].	message = ''		ifTrue: [ message := 'None' ].	^ 'Skills in Cooldown: ' , message! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 01:31'!viewStunned: player	"returns string containing information about stunned situation"	player fight stunned > 0		ifTrue: [ ^ 'Stunned!!' ]		ifFalse: [ ^ 'Not stunned.' ]! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:27'!viewTurnHeader: player	"prints whose turn this turn is"	Transcript		show: player name;		show: '`s turn!!';		cr! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/20/2017 23:06'!viewRemainingPotions: player	"returns string containing information about the numbers of potion types"	| message health mana |	message := ''.	health := player inventory health.	mana := player inventory mana.	health ~= 0		ifTrue: [ message := message , health asString , ' Health' ].	mana ~= 0		ifTrue: [ health ~= 0				ifTrue: [ message := message , ', ' ].			message := message , mana asString , ' Mana' ].	message = ''		ifTrue: [ message := 'None' ].	^ 'Remaining Potions: ' , message! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:18'!viewInventory: player	"prints player inventory"	Transcript		show: player inventory items;		cr! !!GameArena class methodsFor: 'starting' stamp: 'BerkErol 4/27/2017 22:04'!start	"starts the whole game"	| player1 player2 count |	player1 := Player new.	player2 := Player new.	player1 name: 'Player1'.	player2 name: 'Player2'.	player1 fight takeHero: self chooseScourge.	player2 fight takeHero: self chooseSentinel.	self shop: player1.	self shop: player2.	self viewInventory: player1.	self viewInventory: player2.	self useItems: player1.	self useItems: player2.	count := 0.	[ player1 fight hp > 0 and: player2 fight hp > 0 ]		whileTrue: [ count := count + 1.			count % 2 = 1				ifTrue: [ self viewTurnHeader: player1.					self viewPlayers: player1 and: player2.					self fight: player1 with: player2.					self updateCounts: player1 ]				ifFalse: [ self viewTurnHeader: player2.					self viewPlayers: player1 and: player2.					self fight: player2 with: player1.					self updateCounts: player2 ] ].	self viewEnds: player1 and: player2! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/21/2017 00:18'!shop: player	"enables players to buy items and potions"	| list item |	list := self makeShopList: player.	[ player shop slot >= 0.5 and: (player shop gold >= 200 and: list size ~= 0) ]		whileTrue: [ item := self chooseItemForPlayer: player fromList: list.			player inventory addItem: item.			player shop buy: item.			list := self makeShopList: player.			Transcript				show: player shop gold asString;				cr ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/27/2017 22:45'!makeShopList: player	"makes a list from the items and potions that players can buy then returns it"	| list |	list := LinkedList new.	list := self addItem: BattleFury new forPlayer: player toShopList: list.	list := self addItem: Butterfly new forPlayer: player toShopList: list.	list := self addItem: Daedalus new forPlayer: player toShopList: list.	list := self addItem: Desolator new forPlayer: player toShopList: list.	list := self addItem: EnergyBooster new forPlayer: player toShopList: list.	list := self addItem: HealthPotion new forPlayer: player toShopList: list.	list := self addItem: HeartOfTarrasque new forPlayer: player toShopList: list.	list := self addItem: HoodOfDefiance new forPlayer: player toShopList: list.	list := self addItem: ManaPotion new forPlayer: player toShopList: list.	list := self addItem: SkullBasher new forPlayer: player toShopList: list.	list := self addItem: Vanguard new forPlayer: player toShopList: list.	list := self addItem: VitalityBooster new forPlayer: player toShopList: list.	^ list! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/22/2017 00:11'!fight: player1 with: player2	"enables players to fight with each other"	| list message action |	player1 fight stunned > 0		ifTrue: [ ^ self ].	message := player1 name , '`s turn!! (' , player1 fight hero name , ')'.	list := self makeFightList: player1.	action := list		at:			(UIManager default chooseFrom: list lines: #(0) message: message title: 'Choose').	action = 'Attack'		ifTrue: [ player1 fight hero attack: player2.			player1 fight lastAction: 'Attack' ].	action = 'Health Potion'		ifTrue: [ HealthPotion use: player1.			player1 fight lastAction: 'Health Potion' ].	action = 'Mana Potion'		ifTrue: [ ManaPotion use: player1.			player1 fight lastAction: 'Mana Potion' ].	action = player1 fight hero skill1 name		ifTrue: [ player1 fight hero skill1 attack: player1 to: player2.			player1 fight lastAction: player1 fight hero skill1 name.			player1 fight hero skill1 count: player1 fight hero skill1 count + 1 ].	action = player1 fight hero skill2 name		ifTrue: [ player1 fight hero skill2 attack: player1 to: player2.			player1 fight lastAction: player1 fight hero skill2 name.			player1 fight hero skill2 count: player1 fight hero skill2 count + 1 ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/21/2017 20:01'!useItems: player	"enables players to use their items"	player inventory items do: [ :item | item use: player ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/21/2017 22:13'!makeFightList: player	"makes a list for the actions that players can choose for fight then returns it"	| list hero |	list := LinkedList new.	list add: 'Attack'.	player inventory health > 0		ifTrue: [ list add: 'Health Potion' ].	player inventory mana > 0		ifTrue: [ list add: 'Mana Potion' ].	hero := player fight hero.	player fight mana >= hero skill1 mana		ifTrue: [ hero skill1 count = 0				ifTrue: [ hero skill1 type						ifTrue: [ list add: hero skill1 name ] ] ].	player fight mana >= hero skill2 mana		ifTrue: [ hero skill2 count = 0				ifTrue: [ hero skill2 type						ifTrue: [ list add: hero skill2 name ] ] ].	^ list! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 4/27/2017 22:07'!updateCounts: player	"updates hero stunning and skill cooldown counts"	player fight stunned > 0		ifTrue: [ player fight stunned: player fight stunned - 1 ].	player fight hero skill1 count > 0		ifTrue: [ player fight hero skill1 count: player fight hero skill1 count - 1 ].	player fight hero skill2 count > 0		ifTrue: [ player fight hero skill2 count: player fight hero skill2 count - 1 ]! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 4/20/2017 00:28'!chooseScourge	"enables Player 1 to choose a Scourge Hero then returns it"	| list message hero |	list := #('Leshrac' 'Lion' 'Mortred' 'Nyx Assassin' 'Skeleton King' 'Tidehunter').	message := 'Player1: Choose a Scourge hero'.	hero := list at: (UIManager default chooseFrom: list lines: #(0) message: message title: 'Choose').	hero = 'Leshrac'	ifTrue: [ ^ Leshrac new ].	hero = 'Lion'	ifTrue: [ ^ Lion new ].	hero = 'Mortred'	ifTrue: [ ^ Mortred new ].	hero = 'Nyx Assassin'	ifTrue: [ ^ NyxAssassin new ].	hero = 'Skeleton King'	ifTrue: [ ^ SkeletonKing new ].	hero = 'Tidehunter'	ifTrue: [ ^ Tidehunter new ].! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 4/20/2017 23:04'!addItem: item forPlayer: player toShopList: list	"adds an item or a potion to the list"	player shop slot >= item slot		ifTrue: [ player shop gold >= item cost				ifTrue: [ list add: item name ] ].	^ list! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 4/19/2017 23:56'!chooseSentinel	"enables Player 2 to choose a Sentinel Hero then returns it"	| list message hero |	list := #('Earthshaker' 'Magina' 'Mirana' 'Ogre Magi' 'Rylai' 'Sven').	message := 'Player2: Choose a Sentinel hero'.	hero := list at: (UIManager default chooseFrom: list lines: #(0) message: message title: 'Choose').	hero = 'Earthshaker'	ifTrue: [ ^ Earthshaker new ].	hero = 'Magina'	ifTrue: [ ^ Magina new ].	hero = 'Mirana'	ifTrue: [ ^ Mirana new ].	hero = 'Ogre Magi'	ifTrue: [ ^ OgreMagi new ].	hero = 'Rylai'	ifTrue: [ ^ Rylai new ].	hero = 'Sven'	ifTrue: [ ^ Sven new ].! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 4/27/2017 22:44'!chooseItemForPlayer: player fromList: list	"enables players to choose an item then returns it"	| message item |	message := 'Choose an item for ', player name, ' (remaining item slot = ', player shop slot asString, ', remaining gold = ', player shop gold asString, ')'.	item := list at: (UIManager default chooseFrom: list lines: #(0) message: message title: 'Choose').	item = 'Battle Fury'	ifTrue: [ ^ BattleFury new ].	item = 'Butterfly'	ifTrue: [ ^ Butterfly new ].	item = 'Daedalus'	ifTrue: [ ^ Daedalus new ].	item = 'Desolator'	ifTrue: [ ^ Desolator new ].	item = 'Energy Booster'	ifTrue: [ ^ EnergyBooster new ].	item = 'Health Potion'	ifTrue: [ ^ HealthPotion new ].	item = 'Heart of Tarrasque'	ifTrue: [ ^ HeartOfTarrasque new ].	item = 'Hood of Defiance'	ifTrue: [ ^ HoodOfDefiance new ].	item = 'Mana Potion'	ifTrue: [ ^ ManaPotion new ].	item = 'Skull Basher'	ifTrue: [ ^ SkullBasher new ].	item = 'Vanguard'	ifTrue: [ ^ Vanguard new ].	item = 'Vitality Booster'	ifTrue: [ ^ VitalityBooster new ].! !GameArena subclass: #Fight	instanceVariableNames: 'hero hp mana lastAction stunned'	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Fight commentStamp: 'BerkErol 4/19/2017 22:47' prior: 0!Represents fights.!!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/21/2017 01:30'!stunned: newStunned	"sets the stunned of a Fight instance"	stunned := newStunned! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:51'!hero	"returns the hero of a Fight instance"	^ hero! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/21/2017 01:29'!stunned	"returns the stunned of a Fight instance"	^ stunned! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:54'!lastAction: newLastAction	"sets the last action of a Fight instance"	lastAction := newLastAction! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!hp: newHp	"sets the HP of a Fight instance"	hp := newHp! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!mana: newMana	"sets the mana of a Fight instance"	mana := newMana! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!hero: newHero	"sets the hero of a Fight instance"	hero := newHero! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!hp	"returns the HP of a Fight instance"	^ hp! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!mana	"returns the mana of a Fight instance"	^ mana! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:53'!lastAction	"returns the last action of a Fight instance"	^ lastAction! !!Fight methodsFor: 'editing' stamp: 'BerkErol 4/21/2017 01:37'!takeHero: newHero	"sets the variables for the given hero of a Fight instance"	hero := newHero.	hp := newHero hp.	mana := newHero mana.	lastAction := ''.	stunned := 0! !Dota subclass: #Hero	instanceVariableNames: 'name side hp mana armor skill1 skill2 minDamage maxDamage'	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Hero commentStamp: 'BerkErol 4/19/2017 11:16' prior: 0!Represents heroes.!!Hero methodsFor: 'editing' stamp: 'BerkErol 4/20/2017 21:08'!attack: enemyPlayer	"attacks to the enemy"	| givenDamage actualDamage |	givenDamage := (Random new nextInt: maxDamage - minDamage + 1) + minDamage - 1.	actualDamage := (givenDamage * (1 - (0.06 * enemyPlayer fight hero armor / (1 + (0.06 * enemyPlayer fight hero armor)))))		rounded.	enemyPlayer fight hp: enemyPlayer fight hp - actualDamage! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:08'!maxDamage: newMaxDamage	"sets the max damage of a Hero instance"	maxDamage := newMaxDamage! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:08'!minDamage	"returns the min damage of a Hero instance"	^ minDamage! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 21:34'!skill1: newSkill1	"sets the first skill of a Hero instance"	skill1 := newSkill1! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:07'!armor	"returns the armor of a Hero instance"	^ armor! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:08'!armor: newArmor	"sets the armor of a Hero instance"	armor := newArmor! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:01'!hp: newHp	"sets the HP of a Hero instance"	hp := newHp! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:08'!minDamage: newMinDamage	"sets the min damage of a Hero instance"	minDamage := newMinDamage! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 10:48'!name	"returns the name of a Hero instance"	^ name! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:01'!name: newName	"sets the name of a Hero instance"	name := newName! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 21:34'!skill2	"returns the second skill of a Hero instance"	^ skill2! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:08'!mana: newMana	"sets the mana of a Hero instance"	mana := newMana! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:02'!hp	"returns the HP of a Hero instance"	^ hp! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 21:33'!skill1	"returns the first skill of a Hero instance"	^ skill1! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:26'!side: newSide	"sets the side of a Hero instance"	side := newSide! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:07'!mana	"returns the mana of a Hero instance"	^ mana! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:26'!side	"returns the side of a Hero instance"	^ side! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 21:34'!skill2: newSkill2	"sets the second skill of a Hero instance"	skill2 := newSkill2! !!Hero methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 11:07'!maxDamage	"returns the max damage of a Hero instance"	^ maxDamage! !GameArena subclass: #Inventory	instanceVariableNames: 'items health mana'	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Inventory commentStamp: 'BerkErol 4/19/2017 22:27' prior: 0!Represents inventories.!!Inventory methodsFor: 'editing' stamp: 'BerkErol 4/21/2017 00:18'!addItem: item	"adds an item or a potion to an Inventory instance"	item name = 'Health Potion'		ifTrue: [ self health: health + 1 ]		ifFalse: [ item name = 'Mana Potion'				ifTrue: [ self mana: mana + 1 ]				ifFalse: [ items add: item ] ]! !!Inventory methodsFor: 'initialization' stamp: 'BerkErol 4/21/2017 10:25'!initialize	"Creates a new Inventory instance"	super initialize.	items := LinkedList new.	health := 0.	mana := 0! !!Inventory methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 22:47'!health	"returns the number of health potions of an Inventory instance"	^ health! !!Inventory methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 01:50'!items: newItems	"sets the items of an Inventory instance except potions"	items := newItems! !!Inventory methodsFor: 'accessing' stamp: 'BerkErol 4/20/2017 22:48'!mana: newMana	"sets the the num