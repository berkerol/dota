Object subclass: #Dota	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Dota commentStamp: '<historical>' prior: 0!Represents the main class of the game.!Dota subclass: #GameArena	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!GameArena commentStamp: '<historical>' prior: 0!Main class of the game.!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!GameArena class	instanceVariableNames: ''!!GameArena class methodsFor: 'starting' stamp: 'BerkErol 5/17/2017 09:27'!start	"starts the whole game"	| player1 player2 count |	player1 := Player new.	player2 := Player new.	player1 name: 'Player1'.	player2 name: 'Player2'.	player1 fight takeHero: self chooseScourge.	player2 fight takeHero: self chooseSentinel.	self shop: player1.	self shop: player2.	self viewInventory: player1.	self viewInventory: player2.	self useItems: player1.	self useItems: player2.	player1 fight hero skill1 name = 'Blur'		ifTrue: [ player1 effects evasion: player1 effects evasion + 40 ].	count := 0.	[ player1 fight hp > 0 and: player2 fight hp > 0 ]		whileTrue: [ count := count + 1.			count % 2 = 1				ifTrue: [ self viewTurnHeader: player1.					self viewPlayers: player1 and: player2.					self fight: player1 with: player2.					self updateCounts: player1 ]				ifFalse: [ self viewTurnHeader: player2.					self viewPlayers: player1 and: player2.					self fight: player2 with: player1.					self updateCounts: player2 ] ].	self viewEnds: player1 and: player2! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/17/2017 09:35'!skill: effects to: enemyPlayer	"apply active skills' effects to enemy player"	| decHP decMana |	decHP := ((100 - enemyPlayer effects decSkill) * (effects at: 1) / 100) rounded.	decMana := effects at: 2.	enemyPlayer fight hp - decHP > 0		ifTrue: [ enemyPlayer fight hp: enemyPlayer fight hp - decHP ]		ifFalse: [ enemyPlayer fight hp: 0 ].	enemyPlayer fight mana - decMana > 0		ifTrue: [ enemyPlayer fight mana: enemyPlayer fight mana - decMana ]		ifFalse: [ enemyPlayer fight mana: 0 ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/15/2017 22:28'!makeFightList: player	"makes a list for the actions that players can choose for fight then returns it"	| list hero |	list := LinkedList new.	list add: 'Attack'.	player inventory health isEmpty		ifFalse: [ list add: 'Health Potion' ].	player inventory mana isEmpty		ifFalse: [ list add: 'Mana Potion' ].	hero := player fight hero.	player fight mana >= hero skill1 mana		ifTrue: [ hero skill1 count = 0				ifTrue: [ hero skill1 type						ifTrue: [ list add: hero skill1 name ] ] ].	hero skill2 name = 'Pulse Nova'		ifTrue: [ hero skill2 type				ifFalse: [ list add: hero skill2 name ] ]		ifFalse: [ player fight mana >= hero skill2 mana				ifTrue: [ hero skill2 count = 0						ifTrue: [ hero skill2 type								ifTrue: [ list add: hero skill2 name ] ] ] ].	^ list! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/17/2017 09:26'!fight: player1 with: player2	"enables players to fight with each other"	| yesno list message action potion |	player1 fight hero skill2 name = 'Pulse Nova'		ifTrue: [ player1 fight hero skill2 type				ifTrue: [ yesno := LinkedList new.					yesno add: 'Yes'.					yesno add: 'No'.					(yesno at: (UIManager default chooseFrom: yesno message: 'Do you want to deactivate Pulse Nova?' title: 'Pulse Nova')) = 'Yes'						ifTrue: [ player1 fight hero skill2 deactivate ] ] ].	message := player1 name , '`s turn!! (' , player1 fight hero name , ')'.	list := self makeFightList: player1.	action := list at: (UIManager default chooseFrom: list message: message title: 'Choose').	action = 'Attack'		ifTrue: [ player1 fight lastAction: 'Attack'.			player1 fight hero skill1 name = 'Mana Break'				ifTrue: [ player2 fight mana: player2 fight mana - 100 ].			self attack: player1 to: player2 ].	action = 'Health Potion'		ifTrue: [ potion := player1 inventory health removeFirst.			player1 fight hp + potion hp < player1 fight hero hp				ifTrue: [ player1 fight hp: player1 fight hp + potion hp ]				ifFalse: [ player1 fight hp: player1 fight hero hp ].			player1 fight lastAction: 'Health Potion' ].	action = 'Mana Potion'		ifTrue: [ potion := player1 inventory mana removeFirst.			player1 fight mana + potion mana < player1 fight hero mana				ifTrue: [ player1 fight mana: player1 fight mana + potion mana ]				ifFalse: [ player1 fight mana: player1 fight hero mana ].			player1 fight lastAction: 'Mana Potion' ].	action = player1 fight hero skill1 name		ifTrue: [ self skill: (player1 fight hero skill1 use: player2) to: player2.			player1 fight mana: player1 fight mana - player1 fight hero skill1 mana.			player1 fight hero skill1 count: player1 fight hero skill1 count + player1 fight hero skill1 cooldown + 1.			player1 fight lastAction: player1 fight hero skill1 name ].	action = player1 fight hero skill2 name		ifTrue: [ player1 fight hero skill2 name = 'Pulse Nova'				ifTrue: [ player1 fight hero skill2 activate.					player1 fight lastAction: 'Activate Pulse Nova' ]				ifFalse: [ self skill: (player1 fight hero skill2 use: player2) to: player2.					player1 fight mana: player1 fight mana - player1 fight hero skill2 mana.					player1 fight hero skill2 count: player1 fight hero skill2 count + player1 fight hero skill2 cooldown + 1.					player1 fight lastAction: player1 fight hero skill2 name ] ].	player1 fight hero skill2 name = 'Pulse Nova'		ifTrue: [ player1 fight hero skill2 type				ifTrue: [ player1 fight mana - player1 fight hero skill2 mana > 0						ifTrue: [ player2 fight hp - player1 fight hero skill2 damage > 0								ifTrue: [ player2 fight hp: player2 fight hp - player1 fight hero skill2 damage ]								ifFalse: [ player2 fight hp: 0 ].							player1 fight mana: player1 fight mana - player1 fight hero skill2 mana ] ] ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/9/2017 19:55'!updateCounts: player	"updates hero skill cooldown and hp & mana increase from items"	| inc |	player fight hero skill1 count > 0		ifTrue: [ player fight hero skill1 count: player fight hero skill1 count - 1 ].	player fight hero skill2 count > 0		ifTrue: [ player fight hero skill2 count: player fight hero skill2 count - 1 ].	player fight mana + player effects manaAlways < player fight hero mana		ifTrue: [ player fight mana: player fight mana + player effects manaAlways ]		ifFalse: [ player fight mana: player fight hero mana ].	inc := (player fight hp * 7 / 100) rounded * player effects hp7p.	player fight hp + inc < player fight hero hp		ifTrue: [ player fight hp: player fight hp + inc ]		ifFalse: [ player fight hp: player fight hero hp ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/15/2017 20:46'!useItems: player	"enables players to use their items"	player inventory items do: [ :item | item use: player ].	player fight hero minDamage: player fight hero minDamage + player effects minDamage.	player fight hero maxDamage: player fight hero maxDamage + player effects maxDamage.	player fight hero armor: player fight hero armor + player effects armor.	player fight hp: player fight hp + player effects hp.	player fight hero hp: player fight hero hp + player effects hp.	player fight mana: player fight mana + player effects mana.	player fight hero mana: player fight hero mana + player effects mana! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/17/2017 09:11'!attack: player to: enemyPlayer	"attacks to the enemy"	| attack skillMultiply givenDamage actualDamage itemDec skillDec |	(Random new nextInt: 100) < enemyPlayer effects evasion		ifTrue: [ ^ self ].	attack := player fight hero attack.	skillMultiply := (Random new nextInt: 100) < player effects critic		ifTrue: [ attack * 2 ]		ifFalse: [ attack ].	player fight hero skill2 name = 'Enchant Totem'		ifTrue: [ player fight hero skill2 dcount > 0				ifTrue: [ givenDamage := skillMultiply * player fight hero skill2 damage.					player fight hero skill2 dcount: player fight hero skill2 dcount - 1 ]				ifFalse: [ givenDamage := skillMultiply ] ]		ifFalse: [ player fight hero skill2 name = 'Gods Strength'				ifTrue: [ player fight hero skill2 dcount > 0						ifTrue: [ givenDamage := skillMultiply * player fight hero skill2 damage.							player fight hero skill2 dcount: player fight hero skill2 dcount - 1 ]						ifFalse: [ givenDamage := skillMultiply ] ]				ifFalse: [ givenDamage := skillMultiply ] ].	actualDamage := (givenDamage * (1 - (0.06 * enemyPlayer fight hero armor / (1 + (0.06 * enemyPlayer fight hero armor))))) rounded.	itemDec := (Random new nextInt: 100) < enemyPlayer effects dec60		ifTrue: [ actualDamage - 60 ]		ifFalse: [ actualDamage ].	enemyPlayer fight hero skill1 name = 'Anchor Smash'		ifTrue: [ enemyPlayer fight hero skill1 dcount > 0				ifTrue: [ skillDec := itemDec * enemyPlayer fight hero skill1 defense.					enemyPlayer fight hero skill1 dcount: enemyPlayer fight hero skill1 dcount - 1 ]				ifFalse: [ skillDec := itemDec ] ]		ifFalse: [ skillDec := itemDec ].	enemyPlayer fight hp - skillDec > 0		ifTrue: [ enemyPlayer fight hp: enemyPlayer fight hp - skillDec ]		ifFalse: [ enemyPlayer fight hp: 0 ].	enemyPlayer fight hero skill2 name = 'Vampiric Aura'		ifTrue: [ enemyPlayer fight hp: enemyPlayer fight hp + (skillDec * 15 / 100) rounded ]! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/1/2017 11:21'!makeShopList: player	"makes a list from the items and potions that players can buy then returns it"	| list |	list := LinkedList new.	list := self addItem: BattleFury new forPlayer: player toShopList: list.	list := self addItem: Butterfly new forPlayer: player toShopList: list.	list := self addItem: Daedalus new forPlayer: player toShopList: list.	list := self addItem: Desolator new forPlayer: player toShopList: list.	list := self addItem: EnergyBooster new forPlayer: player toShopList: list.	list := self addItem: HealthPotion new forPlayer: player toShopList: list.	list := self addItem: HeartOfTarrasque new forPlayer: player toShopList: list.	list := self addItem: HoodOfDefiance new forPlayer: player toShopList: list.	list := self addItem: ManaPotion new forPlayer: player toShopList: list.	list := self addItem: Vanguard new forPlayer: player toShopList: list.	list := self addItem: VitalityBooster new forPlayer: player toShopList: list.	^ list! !!GameArena class methodsFor: 'editing' stamp: 'BerkErol 5/17/2017 09:37'!shop: player	"enables players to buy items and potions"	| list item |	list := self makeShopList: player.	[ player inventory slot >= 0.5 and: (player inventory gold >= 200 and: list size ~= 0) ]		whileTrue: [ item := self chooseItemForPlayer: player fromList: list.			player inventory buy: item.			player inventory addItem: item.			list := self makeShopList: player.			Transcript				show: 'Remaining gold for ';				show: player name;				show: ' : ';				show: player inventory gold asString;				cr ]! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 22:57'!viewEnds: player1 and: player2	"prints the end of game"	self viewPlayers: player1 and: player2.	player1 fight hp > 0		ifTrue: [ self viewEnd: player1 ]		ifFalse: [ self viewEnd: player2 ]! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 22:36'!viewSkillsInCooldown: player	"returns string containing information about skills in cooldown"	| skill1 skill2 message |	message := ''.	skill1 := player fight hero skill1.	skill1 count > 0		ifTrue: [ message := message , skill1 name , ' (not available for ' , skill1 count asString , ' turns)' ].	skill2 := player fight hero skill2.	skill2 count > 0		ifTrue: [ skill1 count > 0				ifTrue: [ message := message , ', ' ].			message := message , skill2 name , ' (not available for ' , skill2 count asString , ' turns)' ].	message = ''		ifTrue: [ message := 'None' ].	^ 'Skills in Cooldown: ' , message! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:30'!viewPlayers: player1 and: player2	"prints players information"	self viewPlayer: player1.	self viewPlayer: player2! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:18'!viewInventory: player	"prints player inventory"	Transcript		show: player inventory items;		cr! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 5/9/2017 19:32'!viewRemainingPotions: player	"returns string containing information about the numbers of potion types"	| message health mana |	message := ''.	health := player inventory health size.	mana := player inventory mana size.	health ~= 0		ifTrue: [ message := message , health asString , ' Health' ].	mana ~= 0		ifTrue: [ health ~= 0				ifTrue: [ message := message , ', ' ].			message := message , mana asString , ' Mana' ].	message = ''		ifTrue: [ message := 'None' ].	^ 'Remaining Potions: ' , message! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:33'!viewEnd: player	"prints the winner of the game"	Transcript		show: player name;		show: ' (';		show: player fight hero name;		show: ') is the winner of the game !!!!!!'! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 5/15/2017 22:19'!viewPlayer: player	"prints player information"	Transcript		show: player name;		cr;		show: 'Hero: ';		show: player fight hero name;		cr;		show: player fight hp asString;		show: ' / ';		show: player fight hero hp asString;		show: ' HP';		cr;		show: player fight mana asString;		show: ' / ';		show: player fight hero mana asString;		show: ' Mana';		cr;		show: 'Last Action: ';		show: player fight lastAction;		cr;		show: (self viewRemainingPotions: player);		cr;		show: (self viewSkillsInCooldown: player);		cr;		show: player fight hero skill2 type;		cr! !!GameArena class methodsFor: 'displaying' stamp: 'BerkErol 4/21/2017 11:27'!viewTurnHeader: player	"prints whose turn this turn is"	Transcript		show: player name;		show: '`s turn!!';		cr! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 4/29/2017 16:20'!addItem: item forPlayer: player toShopList: list	"adds an item or a potion to the list"	player inventory slot >= item slot		ifTrue: [ player inventory gold >= item cost				ifTrue: [ list add: item name ] ].	^ list! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 5/17/2017 09:38'!chooseScourge	"enables Player 1 to choose a Scourge Hero then returns it"	| list message hero |	list := #('Leshrac' 'Lion' 'Mortred' 'Nyx Assassin' 'Skeleton King' 'Tidehunter').	message := 'Player1: Choose a Scourge hero'.	hero := list at: (UIManager default chooseFrom: list message: message title: 'Choose').	hero = 'Leshrac'		ifTrue: [ ^ Leshrac new ].	hero = 'Lion'		ifTrue: [ ^ Lion new ].	hero = 'Mortred'		ifTrue: [ ^ Mortred new ].	hero = 'Nyx Assassin'		ifTrue: [ ^ NyxAssassin new ].	hero = 'Skeleton King'		ifTrue: [ ^ SkeletonKing new ].	hero = 'Tidehunter'		ifTrue: [ ^ Tidehunter new ]! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 5/17/2017 09:38'!chooseSentinel	"enables Player 2 to choose a Sentinel Hero then returns it"	| list message hero |	list := #('Earthshaker' 'Magina' 'Mirana' 'Ogre Magi' 'Rylai' 'Sven').	message := 'Player2: Choose a Sentinel hero'.	hero := list at: (UIManager default chooseFrom: list message: message title: 'Choose').	hero = 'Earthshaker'		ifTrue: [ ^ Earthshaker new ].	hero = 'Magina'		ifTrue: [ ^ Magina new ].	hero = 'Mirana'		ifTrue: [ ^ Mirana new ].	hero = 'Ogre Magi'		ifTrue: [ ^ OgreMagi new ].	hero = 'Rylai'		ifTrue: [ ^ Rylai new ].	hero = 'Sven'		ifTrue: [ ^ Sven new ]! !!GameArena class methodsFor: 'choosing' stamp: 'BerkErol 5/17/2017 09:39'!chooseItemForPlayer: player fromList: list	"enables players to choose an item then returns it"	| message item |	message := 'Choose an item for ', player name, ' (remaining item slot = ', player inventory slot asString, ', remaining gold = ', player inventory gold asString, ')'.	item := list at: (UIManager default chooseFrom: list message: message title: 'Choose').	item = 'Battle Fury'	ifTrue: [ ^ BattleFury new ].	item = 'Butterfly'	ifTrue: [ ^ Butterfly new ].	item = 'Daedalus'	ifTrue: [ ^ Daedalus new ].	item = 'Desolator'	ifTrue: [ ^ Desolator new ].	item = 'Energy Booster'	ifTrue: [ ^ EnergyBooster new ].	item = 'Health Potion'	ifTrue: [ ^ HealthPotion new ].	item = 'Heart of Tarrasque'	ifTrue: [ ^ HeartOfTarrasque new ].	item = 'Hood of Defiance'	ifTrue: [ ^ HoodOfDefiance new ].	item = 'Mana Potion'	ifTrue: [ ^ ManaPotion new ].	item = 'Vanguard'	ifTrue: [ ^ Vanguard new ].	item = 'Vitality Booster'	ifTrue: [ ^ VitalityBooster new ].! !GameArena subclass: #Fight	instanceVariableNames: 'hero hp mana lastAction'	classVariableNames: ''	poolDictionaries: ''	category: '2014400189'!!Fight commentStamp: 'BerkErol 4/19/2017 22:47' prior: 0!Represents fights.!!Fight methodsFor: 'editing' stamp: 'BerkErol 5/15/2017 23:53'!takeHero: newHero	"sets the variables for the given hero of a Fight instance"	hero := newHero.	hp := newHero hp.	mana := newHero mana.	lastAction := 'None'! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!mana: newMana	"sets the mana of a Fight instance"	mana := newMana! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:54'!lastAction: newLastAction	"sets the last action of a Fight instance"	lastAction := newLastAction! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:51'!hero	"returns the hero of a Fight instance"	^ hero! !!Fight methodsFor: 'accessing' stamp: 'BerkErol 4/19/2017 22:52'!hero: newHero	"sets the hero of a Fight instance"	hero := newHero! !!Fight methodsFor: 'accessing' stamp: 'B